xt "30200,1200,46200,5700"
st "
-- creates an internal reset pulse for initialization/reset
initRst_proc: process(clk_usr, rst)
begin
    if(rst = '1')then
        cnt_init        <= (others => '0');
        rst_i           <= '1';
        fifo_flush_i    <= '1';
    elsif(rising_edge(clk_usr))then
        if(cnt_init < \"010000\")then
            cnt_init        <= cnt_init + 1;
            rst_i           <= '1';
            fifo_flush_i    <= '1';
        elsif(cnt_init >= \"010000\" and cnt_init < \"111111\")then -- first release the flush signal
            cnt_init        <= cnt_init + 1; 
            rst_i           <= '1';
            fifo_flush_i    <= '0';
        elsif(cnt_init = \"111111\")then         -- remain in this state until reset by top
            cnt_init        <= cnt_init;
            rst_i           <= '0';
            fifo_flush_i    <= '0';
        else
            cnt_init        <= (others => '0');
            rst_i           <= '1';
            fifo_flush_i    <= '1';
        end if;
    end if; 
end process;

"
tm "HdlTextMgr"
wrapOption 3
visibleHeight 4600
visibleWidth 17600
)
)
)
