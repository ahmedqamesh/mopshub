st "

//Check FIFO output
assign can_id      = can_id_reg;
assign spi_mon_id  = spi_mon_reg;
assign spi_can_id  = spi_can_reg;
assign spi_power_on_id    = spi_power_on_reg;
assign spi_power_off_id   = spi_power_off_reg;
assign spi_power_id = spi_power_on_id || spi_power_off_id;
always@(*) 
  begin
  // Detect SOP  
  if(cs_eread && data_tra_10bitin == {2'b10,Kchar_sop})// 0x3c = 0011 1100 SOP (if 8b10b)
      begin
        fifo_data_sop    = 1'b1;
        fifo_data_eop    = 1'b0;
        fifo_data_packet = 1'b0;
        can_id_reg       = 1'b0;
        spi_mon_reg      = 1'b0;
        spi_can_reg      = 1'b0; 
        spi_power_on_reg    = 1'b0;
        spi_power_off_reg   = 1'b0;
      end 
  // Detect DATA  
  else if(cs_eread && data_tra_10bitin != {2'b10,Kchar_sop}&& data_tra_10bitin != {2'b01,Kchar_eop} && data_tra_10bitin[9:8] == 2'b00)// data
      begin
        fifo_data_sop    = 1'b0;
        fifo_data_eop    = 1'b0;
        fifo_data_packet = 1'b1;
        if (addr_read ==5'h02)//detect the first Byte
          case(data_tra_10bitin[7:0])
            8'h20 : //Identify SPI mon
            begin
              can_id_reg    = 1'b0;
              spi_power_on_reg    = 1'b0;
              spi_power_off_reg   = 1'b0;
              if (dbg_spi) // Measurement Board 
                begin 
                  spi_can_reg   = 1'b1; 
                  spi_mon_reg   = 1'b0;
                end
              else // MOPSHUB Board 
                begin 
                  spi_mon_reg   = 1'b1; // I switched them here for debuging
                  spi_can_reg   = 1'b0; 
                end
            end
            8'h90 : //Identify SPI powering off
            begin
              spi_mon_reg   = 1'b0;
              spi_can_reg   = 1'b0;
              can_id_reg    = 1'b0;
              spi_power_on_reg   = 1'b0;
              spi_power_off_reg = 1'b1;
            end
            8'h91 : //Identify SPI powering on
            begin
              spi_mon_reg   = 1'b0;
              spi_can_reg   = 1'b0;
              can_id_reg    = 1'b0;
              spi_power_on_reg = 1'b1;
              spi_power_off_reg   = 1'b0;
            end
//            8'h00 :
//            begin  //Identify CAN not needed now 
//              can_id_reg     = 1'b1;
//              spi_mon_reg    = 1'b0;
//              spi_can_reg    = 1'b0;
//              spi_power_on_reg  = 1'b1;
//              spi_power_off_reg = 1'b0;
//            end
            default:
            begin
              can_id_reg    = 1'b1;
              spi_mon_reg   = 1'b0;
              spi_can_reg   = 1'b0;
              spi_power_on_reg    = 1'b0;
              spi_power_off_reg   = 1'b0;
              end
            endcase
        else
          begin
            can_id_reg    = 1'b0;
            spi_mon_reg   = 1'b0;
            spi_can_reg   = 1'b0;
            spi_power_on_reg    = 1'b0;
            spi_power_off_reg   = 1'b0;
          end                    
      end 
  // Detect EOP  
  else if(cs_eread&&  data_tra_10bitin == {2'b01,Kchar_eop})// 0xdc = 1101 1100 EOP(if 8b10b)
      begin
       fifo_data_sop    = 1'b0;
       fifo_data_eop    = 1'b1;
       fifo_data_packet = 1'b0; 
       can_id_reg       = 1'b0;
       spi_mon_reg      = 1'b0;
       spi_can_reg      = 1'b0;
       spi_power_on_reg    = 1'b0;
       spi_power_off_reg   = 1'b0;       
      end
  
  else  
    begin
     can_id_reg       = 1'b0;
     spi_can_reg      = 1'b0;
     spi_mon_reg      = 1'b0;
     spi_power_on_reg    = 1'b0;
     spi_power_off_reg   = 1'b0;
     fifo_data_sop    = 1'b0;
     fifo_data_eop    = 1'b0;
     fifo_data_packet = 1'b0;
    end    
  end









































































































































































































