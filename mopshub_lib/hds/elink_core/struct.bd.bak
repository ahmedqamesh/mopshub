//Check FIFO output
always@(*)//data_tra_10bitin or cs_eread)
  begin
  if(data_tra_10bitin == {2'b10,Kchar_sop})// 0x3c = 0011 1100 SOP (if 8b10b)
      begin
        fifo_data_sop = 1'b1;
        fifo_data_eop = 1'b0;
        fifo_data_packet =1'b0;
      end 
    else if(cs_eread && data_tra_10bitin != {2'b10,Kchar_sop}&& data_tra_10bitin != {2'b01,Kchar_eop} && data_tra_10bitin[9:8] == 2'b00)// data
      begin
        fifo_data_sop = 1'b0;
        fifo_data_eop = 1'b0;
        fifo_data_packet = 1'b1;
        if (addr_read ==5'h02)//detect the first Byte
          case(data_tra_10bitin[7:0])
            8'h060 : can_id = 1;
            8'h070 : can_id = 1;
            8'h080 : spi_id = 1;
            default:
            begin
              can_id = 1;
              spi_id = 0;
              end
            endcase
        else
          begin
            can_id = 1;
            spi_id = 0;
            end        
            
      end   
    else if(data_tra_10bitin == {2'b01,Kchar_eop})// 0xdc = 1101 1100 (if 8b10b)
      begin
       fifo_data_sop = 1'b0;
       fifo_data_eop = 1'b1;
       fifo_data_packet =1'b0;
      end
    else  
      begin
       fifo_data_sop = 1'b0;
       fifo_data_eop = 1'b0;
       fifo_data_packet =1'b0;
      end    
  end










































































































































"
