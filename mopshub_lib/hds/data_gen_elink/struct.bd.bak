// data_loader 1
//sel din process
always @(posedge genCLK)
begin 
  if(rst == 1 | loop_en == 1)
  case (addr)
    5'b0 : begin 
      dout_r  <= 8'h00;
      delimeter_r  <= 2'b11;
    end
    5'b00001 : begin 
      dout_r  <= 8'h00;
      delimeter_r  <= 2'b10;
    end
    5'b00010 : begin 
      dout_r  <= 8'hDE;
      delimeter_r  <= 2'b00;
    end
    5'b00011 : begin 
      dout_r  <= 8'hAD;
      delimeter_r <= 2'b00;
    end
    5'b00100 : begin 
      dout_r  <= 8'hBE;
      delimeter_r  <= 2'b00;
    end
    5'b00101 : begin 
      dout_r  <= 8'hEF;
      delimeter_r <= 2'b00;
    end
    5'b00110 : begin 
      dout_r  <=8'hDE;
      delimeter_r  <=2'b00;
    end
    5'b00111 : begin 
      dout_r  <= 8'hAD;
      delimeter_r  <= 2'b00;
    end
    5'b01000 : begin 
      dout_r  <= 8'hBE;
      delimeter_r  <= 2'b00;
    end
    5'b01001 : begin 
      dout_r  <= 8'hEF;
      delimeter_r <= 2'b00;
    end
    5'b01010 : begin 
      dout_r  <= 8'hDE;
      delimeter_r <= 2'b00;
    end
    5'b01011 : begin 
      dout_r  <= {4'hA,4'hF};
      delimeter_r <= 2'b00;

    end  
    5'b01100 : begin 
      dout_r  <=8'h00;
      delimeter_r  <=2'b01;
    end
    default : begin 
      dout_r  <=8'h0;
      delimeter_r  <=2'b11;
    end
    
  endcase
  else
  begin
    dout_r <= 8'h0;
    delimeter_r  <=2'b11;
  end
end                                            












































