// data_loader 1
//sel din process
always @(posedge clk_usr)
 begin 
  if(enable == 1 | loop_en == 1)
    case (sel_cnt)
        2'b00 : begin 
                 dout_i  <= 16'h10;
                 delimeter_r  <= 2'b10;
                end
        2'b01 : begin 
                dout_i  <= 16'hDEAD;
                delimeter_r  <= 2'b00;
                end
        2'b10 : begin 
                 dout_i  <= 16'hBEEF;
                 delimeter_r <= 2'b00;
                end
        2'b11 : begin 
                 dout_i  <=16'h01;
                 delimeter_r  <=2'b01;
                end
    endcase
  else
  begin
    dout_i <= 16'h0;
    delimeter_r  <=2'b11;
  end
end                                            








