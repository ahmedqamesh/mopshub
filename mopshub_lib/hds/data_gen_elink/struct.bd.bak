// data_loader 1
//sel din process
assign gen_edata_10bit ={delimeter_reg,gen_edata_8bit_reg}; 
assign reset = !rst;  
always @(posedge genCLK)
begin 
  if(rst == 1 | loop_en == 1)
  case (addr)
    5'b0 : begin 
