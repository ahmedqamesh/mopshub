assign comma_valid_bits[0] = (word10b_align_array[0] == COMMAp | word10b_align_array[0]== COMMAn) ? 1:0;
assign comma_valid_bits[1] = (word10b_align_array[1] == COMMAp | word10b_align_array[1]== COMMAn) ? 1:0;

















