// ISKcode_asign 2  
initial busy_r = 1'b0;                                      
assign ISK_comma   = (word10b == COMMAp |  word10b == COMMAn )  ? 1: 0;
assign ISK_soc     = (word10b == SOCp   |  word10b == SOCn   )  ? 1: 0;
assign ISK_eoc     = (word10b == EOCp   |  word10b == EOCn   )  ? 1: 0;
