xt "8200,6200,24300,10700"
st "
initial zeros2bit   = 2'b0;
initial send_count  = 1'b0;
initial bit_cnt     = 1'b0;    
assign enc_10b_data_out = enc_10b_data_out_r;

always @(posedge clk)
  if (!rst) bit_cnt <= 1'b0;
  else  bit_cnt <= ! bit_cnt;
    
always @ (posedge clk)
  if (!rst) 
    begin
      if (bit_cnt ==1) enc_10b_data_out_r <=COMMAp;
      else enc_10b_data_out_r <=COMMAn;
    end
  else 
    begin
      if (data_10bit_in[7:0] == Kchar_comma) 
        begin
          if (bit_cnt ==1) enc_10b_data_out_r <=COMMAp;
          else enc_10b_data_out_r <=COMMAn;
        end
      else enc_10b_data_out_r <= data_out; 
    end
  
//Fill the register enc10bit_r with data 
always @(posedge clk)
begin
  if (!rst)
    begin
      if (bit_cnt ==1) enc10bit_r <=COMMAp;
      else enc10bit_r <=COMMAn;
    end
  //Trigger after cycle 5 CLKs
  else if (get_data_trig  == 1) 
  begin
    enc10bit_r <= enc_10b_data_out;
  end
end

//-------------------------------------------------------------------------------------------
//-- sending out 2 bits @ bitCLK using a MUX
//-------------------------------------------------------------------------------------------
// Counter over the select signal
always @(posedge clk)
begin
  if (!rst)
