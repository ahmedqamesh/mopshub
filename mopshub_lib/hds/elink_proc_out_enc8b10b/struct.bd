  //Fill the register enc10bit_r with data 
always @(posedge clk)
begin
  if (!rst)
  enc10bit_r <= COMMAn;
  else if (get_data_trig  == 1) //Trigger after cycle 5 CLKs
  begin
    enc10bit_r <= enc10bit;
  end
end

//-------------------------------------------------------------------------------------------
//-- sending out 2 bits @ bitCLK using a MUX
//-------------------------------------------------------------------------------------------
// Counter over the select signal
always @(posedge clk)
begin
  if (!rst)
  send_count <= 3'b0;
  else
  begin
    if (get_data_trig == 1)
    send_count <= 3'b0;
    else  
    send_count <= send_count + 1;
  end
end                                   































































