// HDL Embedded Text Block 5 eb5
// HDL Embedded Text Block 4 data_reg
  //Fill the register enc10bit_r with data 
  assign reset = !rst;
  assign send_out_trig =(sel ==3'b100)? 1:0;
  always @(posedge clk)
  begin
    if (rst ==1)
    enc10bit_r <= 10'b0;
    else if (send_out_trig  == 1) //Trigger after cycle 5 CLKs
    begin
      if (reverse_stream_10b  == 0)//LSB send first 
      enc10bit_r <= {enc10bit[0],enc10bit[1],enc10bit[2],enc10bit[3],enc10bit[4],enc10bit[5],enc10bit[6],enc10bit[7],enc10bit[8],enc10bit[9]};
      
      else //MSB send first 
      enc10bit_r <= enc10bit;
    end
  end
//delay the reset signal tell the last two bits are transferred
always @(posedge clk)
begin
  if (rst ==1)
  begin
    send_trig1 <= 1'b0;
    send_trig2 <= 1'b0;
    bit_cnt_rst <= 1'b0;
  end
  else 
  begin
  if (send_out_trig  == 1) 
  begin
    send_trig1 <= send_out_trig;
    send_trig2 <= send_trig1;
    bit_cnt_rst <= send_trig2;
  end
  else  
  begin
    send_trig1 <= 1'b0;
    send_trig2 <= send_trig1;
    bit_cnt_rst <= send_trig2;
  end
  end
end
















































