st "
assign spi_id     = data_tra_spi_out[75:68];
assign spi_select = data_tra_spi_out[67:60];
assign spi_reg    = data_tra_spi_out[59:52];

assign SPI_MODE     = 1'b0;
initial end_cnt = 1'b0;
initial clk_en      = 1'b1;
initial buffer_en      = 1'b0;


assign bus_cnt_active = (start_init ==1)? bus_cnt : spi_select[4:0];
// Counter end_osc_cnt over the Clock signal
always @(posedge clk)
begin
  if (!rst) end_cnt <= 0;
  else 
    case(bus_cnt)
     n_buses: end_cnt <= 1;
     default: end_cnt <= 0;
    endcase
end



//FIFO block
always @(posedge clk)
begin
  if (!rst)
    begin 
      rena <= 0;
      buffer_en <=0;
    end
  else if (!fifo_empty) 
    begin 
      rena <= 1;
      buffer_en <=1;
    end
  else
      begin 
      rena <= 0;
      buffer_en <=0;
    end
end








































